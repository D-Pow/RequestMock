{
  "name": "mock-requests",
  "version": "1.2.1",
  "description": "Mocks XMLHttpRequest and fetch requests with the specified response object",
  "author": "Devon Powell",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.js",
  "files": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/D-Pow/MockRequests.git"
  },
  "scripts": {
    "test": "jest --coverage tests",
    "build": "webpack --mode production",
    "postbuild": "npm run jsdoc",
    "jsdoc": "jsdoc -R ./ReadMe.md -c config/jsdoc.config.json"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/plugin-transform-template-literals": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.7.7",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^6.0.3",
    "coveralls": "^3.0.6",
    "docdash": "^1.1.1",
    "jest": "^25.1.0",
    "jsdoc": "^3.6.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/jest.config.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist",
      "<rootDir>/demo"
    ]
  },
  "keywords": [
    "network",
    "mock",
    "mocking",
    "mocks",
    "mocker",
    "fetch",
    "fetch-mock",
    "XMLHttpRequest",
    "XMLHttpRequest-mock",
    "request",
    "requests",
    "request-mock",
    "XHR",
    "xhr-mock",
    "AJAX",
    "ajax mock",
    "HTTP",
    "HTTPS"
  ]
}
